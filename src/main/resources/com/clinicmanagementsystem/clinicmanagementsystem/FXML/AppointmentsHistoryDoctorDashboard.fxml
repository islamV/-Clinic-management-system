<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.clinicmanagementsystem.clinicmanagementsystem.AppointmentsHistoryDoctorDashboard">
    <TableView fx:id="tableView" layoutX="20.0" layoutY="20.0" prefHeight="400.0" prefWidth="760.0">
        <columns>
            <TableColumn fx:id="appointmentIdColumn" text="Appointment ID" prefWidth="50.0" />
            <TableColumn fx:id="patientNameColumn" text="Patient Name" prefWidth="100.0" />
            <TableColumn fx:id="doctorNameColumn" text="Doctor Name" prefWidth="100.0" />
            <TableColumn fx:id="queueNumberColumn" text="Queue Number" prefWidth="100.0" />
            <TableColumn fx:id="statusColumn" text="Status" prefWidth="100.0" />
            <TableColumn fx:id="scheduleDayColumn"  text="Day" prefWidth="100.0" />
            <TableColumn fx:id="createdAtColumn"  text="Created at" prefWidth="150.0" />


        </columns>
    </TableView>
    <Button text="Back" layoutX="20.0" layoutY="440.0" onAction="#handleBackButton" />
</AnchorPane>
